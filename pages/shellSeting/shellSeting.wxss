Page{
    background: black;
}

.contain{
    animation: upp 3s cubic-bezier(0.19, 1, 0.22, 1)  forwards;
  }
  
  @keyframes upp{
    0% {opacity: 0;transform: translate3d(0,100vh,0);}
    50% {opacity: 1;}
    100% {opacity: 1;transform: translate3d(0,0,0);}
  }

.buttonred.act{
    background:rgb(255, 97, 97);
